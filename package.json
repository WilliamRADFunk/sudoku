{
  "name": "sudoku",
  "version": "0.1.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve example --disableHostCheck=true",
    "build": "run-s lint test:sudoku clean build:lib test:example build:example copy:configs",
    "build:example": "ng build example --prod --base-href=/applications/sudoku/",
    "build:lib": "ng build sudoku",
    "clean": "rimraf dist/",
    "copy:configs": "cpx projects/example/src/assets/configs/.htaccess dist/example/",
    "test": "ng test --code-coverage --watch=false",
    "test:example": "ng test example --code-coverage --watch=false",
    "test:sudoku": "ng test sudoku --code-coverage --watch=false",
    "test:watch": "ng test --code-coverage",
    "lint": "ng lint",
    "e2e": "ng e2e"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~7.1.0",
    "@angular/common": "~7.1.0",
    "@angular/compiler": "~7.1.0",
    "@angular/core": "~7.1.0",
    "@angular/forms": "~7.1.0",
    "@angular/platform-browser": "~7.1.0",
    "@angular/platform-browser-dynamic": "~7.1.0",
    "@angular/router": "~7.1.0",
    "@ng-bootstrap/ng-bootstrap": "^4.0.1",
    "bignumber.js": "^8.1.1",
    "bootstrap": "^4.2.1",
    "core-js": "^2.5.4",
    "file-saver": "^2.0.1",
    "font-awesome": "^4.7.0",
    "rxjs": "~6.3.3",
    "tslib": "^1.9.0",
    "zone.js": "~0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.11.0",
    "@angular-devkit/build-ng-packagr": "~0.11.0",
    "@angular/cli": "~7.1.4",
    "@angular/compiler-cli": "~7.1.0",
    "@angular/language-service": "~7.1.0",
    "@types/jasmine": "~2.8.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "codelyzer": "~4.5.0",
    "cpx": "^1.5.0",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~3.1.1",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~1.1.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "ng-packagr": "^10.1.1",
    "npm-run-all": "^4.1.5",
    "protractor": "~5.4.0",
    "rimraf": "^2.6.3",
    "ts-node": "~7.0.0",
    "tsickle": ">=0.29.0",
    "tslib": "^1.9.0",
    "tslint": "~5.11.0",
    "typescript": "~3.1.6",
    "webpack-dev-server": "^3.1.11"
  }
}
